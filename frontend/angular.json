{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "schematics": {
        "@schematics/angular": {
            "component": {
                "changeDetection": "OnPush"
            }
        }
    },
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "excel-tool": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "inlineTemplate": false,
                    "inlineStyle": false,
                    "style": "scss",
                    "skipTests": true
                },
                "@schematics/angular:class": {
                    "skipTests": true
                },
                "@schematics/angular:directive": {
                    "skipTests": true
                },
                "@schematics/angular:guard": {
                    "skipTests": true
                },
                "@schematics/angular:interceptor": {
                    "skipTests": true
                },
                "@schematics/angular:pipe": {
                    "skipTests": true
                },
                "@schematics/angular:service": {
                    "skipTests": true
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.json",
                        "aot": true,
                        "assets": [
                            "src/favicon.ico",
                            "src/assets",
                            {
                                "glob": "**/*",
                                "input": "./node_modules/@ant-design/icons-angular/src/inline-svg/",
                                "output": "/assets/"
                            },
                            {
                                "glob": "**/*",
                                "input": "./projects/workstation-shared/assets/",
                                "output": "/assets/"
                            }
                        ],
                        "styles": [
                            "src/theme.less",
                            "src/styles.scss"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "tsConfig": "tsconfig.prod.json",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "port": 3102,
                        "browserTarget": "excel-tool:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "excel-tool:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "excel-tool:build"
                    }
                }
            }
        },
        "workstation-core": {
            "projectType": "library",
            "root": "projects/workstation-core",
            "sourceRoot": "projects/workstation-core/src",
            "prefix": "workstation-core",
            "schematics": {
                "@schematics/angular:component": {
                    "inlineTemplate": false,
                    "inlineStyle": false,
                    "style": "scss",
                    "skipTests": true
                },
                "@schematics/angular:class": {
                    "skipTests": true
                },
                "@schematics/angular:guard": {
                    "skipTests": true
                }
            },
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "project": "projects/workstation-core/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/workstation-core/tsconfig.lib.prod.json"
                        },
                        "development": {
                            "tsConfig": "projects/workstation-core/tsconfig.lib.json"
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/workstation-core/src/test.ts",
                        "tsConfig": "projects/workstation-core/tsconfig.spec.json",
                        "karmaConfig": "projects/workstation-core/karma.conf.js"
                    }
                }
            }
        },
        "workstation-shared": {
            "projectType": "library",
            "root": "projects/workstation-shared",
            "sourceRoot": "projects/workstation-shared/src",
            "prefix": "workstation-shared",
            "schematics": {
                "@schematics/angular:component": {
                    "inlineTemplate": false,
                    "inlineStyle": false,
                    "style": "scss",
                    "skipTests": true
                },
                "@schematics/angular:class": {
                    "skipTests": true
                },
                "@schematics/angular:guard": {
                    "skipTests": true
                }
            },
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "project": "projects/workstation-shared/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/workstation-shared/tsconfig.lib.prod.json"
                        },
                        "development": {
                            "tsConfig": "projects/workstation-shared/tsconfig.lib.json"
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/workstation-shared/src/test.ts",
                        "tsConfig": "projects/workstation-shared/tsconfig.spec.json",
                        "karmaConfig": "projects/workstation-shared/karma.conf.js"
                    }
                }
            }
        }
    },
    "defaultProject": "excel-tool",
    "cli": {
        "analytics": false
    }
}